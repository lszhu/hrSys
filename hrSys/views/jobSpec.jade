.text-center
  label.radio-inline <strong>在家务农</strong>
    input(type="radio", name="employment", value="0", checked="checked")
  label.radio-inline <strong>在外务工</strong>
    input(type="radio", name="employment", value="1")
  label.radio-inline <strong>自主创业</strong>
    input(type="radio", name="employment", value="2")
hr
.col-sm-12
  .input-group
    span.input-group-addon 是否打算外出就业
    select.form-control(name="workTrend")
      option(value="1") 是
      option(value="0") 否
    span.input-group-addon 外出就业意向
    select.form-control(name="jobPreference")
      option(value="0") 自主创业（经商）
      option(value="1") 务工就业
      option(value="2") 其他
.col-sm-12
  .input-group
    span.input-group-addon 打算返乡
    select.form-control(name="project")
      optgroup(label="否")
        option(value="0") 否
      optgroup(label="是")
        option(value="1") 返乡务农
        option(value="2") 务工就业
        option(value="3") 自主创业
        option(value="4") 其他
    span.input-group-addon 外出务工月收入
    select.form-control(name="salary")
      option(value="0") 1500元以下
      option(value="1") 1500-2000元
      option(value="2") 2000-3000元
      option(value="3") 3000元以上
      option(value="4") 不详
    span.input-group-addon 外出务工时间
    select.form-control(name="workExperience")
      option(value="0") 半年以下
      option(value="1") 半年以上
      option(value="2") 一年以上
      option(value="3") 二年以上
      option(value="4") 三年以上
      option(value="5") 五年以上
      option(value="6") 十年以上
