.text-center
  label.radio-inline <strong>在家务农</strong>
    input(type="radio", name="job", value="0", checked="checked")
  label.radio-inline <strong>在外务工</strong>
    input(type="radio", name="job", value="1")
  label.radio-inline <strong>自主创业</strong>
    input(type="radio", name="job", value="2")
